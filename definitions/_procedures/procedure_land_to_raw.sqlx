CREATE OR REPLACE PROCEDURE
  `PROCEDURES.LOAD_LAND_TO_RAW`(tableName STRING)
BEGIN
DECLARE
  InsertFull,
  InsertIncr,
  ConditionId,
  PartitionId STRING;
DECLARE
  Result,
  PartitionIdResult INT64;
SET  ConditionId = FUNCTIONS.FncCheckTable2(tableName);
SET  PartitionId = FUNCTIONS.FncGetRawPartitions2(tableName);
SET  PartitionIdResult = CAST(PartitionId AS INT64);
SET  Result = CAST(ConditionId AS INT64);
SET  InsertFull = "INSERT INTO `RAW."||tableName||"` SELECT  *,  _PARTITIONTIME AS PARTITIONTIME FROM  `LAND."||tableName||"`";
SET  InsertIncr = "INSERT INTO `RAW."||tableName||"` WITH TABELA AS (SELECT DISTINCT *, _PARTITIONTIME AS PARTITIONTIME FROM `LAND."||tableName||"`),TABELA_FILTER AS (SELECT *, CAST(REPLACE(REPLACE(SUBSTRING(CAST(PARTITIONTIME AS STRING),1,13),' ',''),'-','') AS INT64) AS FILTRO FROM TABELA ) SELECT * EXCEPT (FILTRO) FROM  TABELA_FILTER WHERE  FILTRO > "||PartitionIdResult||"";
# CAST(REPLACE(REPLACE(SUBSTRING(CAST(PARTITIONTIME AS STRING),1,13),' ',''),'-','') AS INT64) AS FILTRO
# CAST(REPLACE(CAST(DATE(PARTITIONTIME) AS STRING),'-','') AS INT64) AS FILTRO

IF
  Result=0 THEN
    EXECUTE IMMEDIATE InsertFull;
  ELSE
    EXECUTE IMMEDIATE InsertIncr;
END IF
  ;
END
  ;
