config {
    tags: ["cubo-estoque"]
}

INSERT INTO 
    REFINED.ESTOQUE_HISTORICO (
SELECT
    COD_MATERIAL,
    COD_FILIAL,
    COD_DEPOSITO,
    CAST(CONCAT(DAT_ESTOQUE, ':00') AS TIMESTAMP) AS DAT_ESTOQUE,
    CAST(QTD_DISPONIVEL AS INT64) AS QTD_DISPONIVEL,
    CAST(QTD_TRANSITO AS INT64) AS QTD_TRANSITO,
    VAL_PMM,
    VAL_PMM_AJUSTADO,
    DATA_ATUALIZACAO,
    BANDEIRA
FROM CUBO_ESTOQUE.ESTOQUE_ONLINE
);